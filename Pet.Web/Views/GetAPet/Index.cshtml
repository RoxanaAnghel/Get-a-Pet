
@{
    Layout = null;
}
@using Microsoft.AspNet.Identity
<!DOCTYPE html>
<html ng-app="getAPet">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Get a pet</title>
    @Styles.Render("~/Content/mdl")
    @Styles.Render("~/angular-material")
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

</head>
<body>



    @*<div ng-controller="AppCtrl" ng-cloak>*@
        <md-content class="md-padding">
            <md-nav-bar md-selected-nav-item="currentNavItem"
                        nav-bar-aria-label="navigation links">

                <md-nav-item md-nav-href="#home">Home</md-nav-item>
                <md-nav-item md-nav-href="#mypets">MyPets</md-nav-item>
                <md-nav-item md-nav-href="#userconversations">Messages</md-nav-item>
                <md-nav-item md-nav-href="#profile">Profile</md-nav-item>


            </md-nav-bar>
            <div class="ext-content">
            </div>
        </md-content>
    @*</div>*@



        <div class="container body-content">
            <div ng-view>

            </div>

            <footer style="clear:both">
                <hr />
                <p>&copy; @DateTime.Now.Year - Get a Pet</p>
            </footer>
        </div>
        @Scripts.Render("~/angular")
        @Scripts.Render("~/module")
        @Scripts.Render("~/AngularControllers")
</body>
</html>
